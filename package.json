{
  "name": "nevadacaldwell.com",
  "version": "1.1.0",
  "description": "Personal actor website for Nevada Caldwell.",
  "main": "index.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "NODE_ENV=development npm-run-all --serial build:assets --parallel serve watch:*",
    "serve": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "chokidar 'src/_assets/css/**/*' 'tailwind.config.js' --command 'run-s build:assets:css'",
    "watch:js": "rollup -c -w",
    "watch:svg": "chokidar 'src/_assets/svg/**/*' --command 'run-s build:assets:svg'",
    "build": "NODE_ENV=production npm-run-all --sequential build:assets build:11ty",
    "build:assets": "npm-run-all --parallel build:assets:*",
    "build:assets:css": "postcss src/_assets/css/index.css -o _site/css/styles.css",
    "build:assets:js": "rollup -c",
    "build:assets:svg": "svg-sprite --svg-xmldecl=false --svg-doctype=false --symbol=true --symbol-inline=true --symbol-dest=src/_includes src/_assets/svg/*.svg",
    "build:11ty": "ELEVENTY_ENV=production npx eleventy",
    "postinstall": "netlify-lambda install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "marked": "^0.8.2",
    "smooth-scroll": "cferdinandi/smooth-scroll"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "autoprefixer": "^9.7.5",
    "chokidar-cli": "^2.1.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "purgecss": "^2.1.0",
    "rollup": "^2.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "svg-sprite": "^1.5.0",
    "tailwindcss": "^1.2.0"
  }
}
